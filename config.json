{
  "data_labeling" : {
    "use_gemini":  true,
    "search_github": true,
    "search_zenodo": true,
    "search_lod_cloud": true,
    "stop_before_merging": false
  },
  "data-extraction": {
    "start-offset": 0,
    "limit": 4000,
    "save_range": 16,
    "extract_sparql": true,
    "query_lov": true
  },
  "processing" : {
    "use_ner": true,
    "use_filter": true
  },
  "training" : {
     "classifier": "NB",
     "feature": ["CURI", "PURI"],
     "oversample": false,
     "max_token": 256,
     "use_tfidf_autoencoder": true
  },
  "profile": {
    "store_profile_after_training": false,
    "store_profile_on_run": true,
    "base_domain": "https://exemple.org"
  },
  "general_setting": {
    "info": "",
    "start_phase": "data_labeling",
    "stop_phase": "training"
  }
}